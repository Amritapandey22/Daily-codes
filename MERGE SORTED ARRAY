/* LEETCODE 88. Merge Sorted Array */

class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        /*int i,j=0;
        for(i=m;i<nums1.size();i++){
            nums1[i]=nums2[j];
            j++;
        }*/
        copy(nums2.begin(), nums2.begin() + n, nums1.begin() + m);
        sort(nums1.begin(), nums1.end());
        //sort(nums1.begin(),nums1.end());
        
        
    }
};
